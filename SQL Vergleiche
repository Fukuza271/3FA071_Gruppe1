CREATE TABLE Customer(
Customer_UUID int NOT NULL AUTO_INCREMENT PRIMARY KEYcustomer,
FirstName varchar(50) NOT NULL,
LastName varchar(50) NOT NULL,
BirthDay DateTime,
Gender ENUM('D','M','U','W') NOT NULL
)

CREATE TABLE Reading(
Reading_UUID int NOT NULL AUTO_INCREMENT,
Comment varchar(100),
DateOfReading DateTime, 
KindOfMeter ENUM('HEIZUNG','STROM','WASSER','UNBEKANNT') NOT NULL,
)
